    "import gym\n",
    "import highway_env\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "env = gym.make(\"racetrack-v0\")\n",
    "env.reset()\n",
    "for _ in range(3):\n",
    "    action = [0.1,0.1]\n",
    "    obs, reward, done, info = env.step(action)\n",
    "    env.render()\n",
    "\n",
    "plt.imshow(env.render(mode=\"rgb_array\"))\n",
    "plt.show()"
   ]
